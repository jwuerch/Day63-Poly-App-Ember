
<div class="save-user-form">
  <form>

    <div class="form-group">
      <label for="username">Username</label>
      {{input value=username id="username"}}
    </div>
    <div class="form-group">
      <label for="birth_month">Birthday</label>
      {{input value=birth_month id="birth_month"}}
    </div>
    <div class="form-group">
      <label for="zip_code">Zip Code</label>
      {{input value=zip_code id="zip_code"}}
    </div>
    <div class="form-group">
      <label for="country">Country</label>
      {{input value=country id="country"}}
    </div>
    <button {{action 'saveUser'}}>Next</button>
  </form>
</div>
